package May30;

import java.lang.reflect.Array;
import java.util.Arrays;

public class CollectionsFramework {
    public static void main(String[] args) {
        System.out.println();
        int[] arr = new int[5];
        Arrays.fill(arr, 5); // 99999
        Arrays.setAll(arr, i -> (int) (Math.random()*5)); //랜덤값 5개
        
    }
}
